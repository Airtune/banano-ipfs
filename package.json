{
  "name": "banano-ipfs",
  "version": "1.0.0",
  "description": "Tool for converting between IPFS CID v0, hex, and Banano addresses.",
  "main": "index.js",
  "author": "Airtune and Coranos",
  "license": "MIT",
  "keywords": [
    "banano"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BananoCoin/banano-ipfs.git"
  },
  "files": [
    "./index.js"
  ],
  "scripts": {
    "coverage": "nyc -x 'test/util/**' -x 'test/**' ./node_modules/mocha/bin/mocha -t 100000 test/",
    "docs": "./node_modules/documentation/bin/documentation.js build --config documentation.yaml documentation.js -f md -o docs/documentation.md",
    "eslint": "eslint --fix 'index.js';eslint --fix 'test/**/*.js'",
    "publish-preflight": "npm outdated;npm i;npm audit fix;npm run eslint;npm run coverage;npm run docs;",
    "test": "./node_modules/mocha/bin/mocha --recursive -t 100000 test/"
  },
  "dependencies": {
    "@bananocoin/bananojs": "^2.4.15",
    "bs58": "^4.0.1"
  },
  "preferGlobal": true,
  "devDependencies": {
    "chai": "^4.3.4",
    "documentation": "^13.2.5",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0"
  }
}
