{
  "name": "@bananocoin/banano-ipfs",
  "version": "1.1.0",
  "description": "Tool for converting between IPFS CID v0, hex, and Banano addresses.",
  "main": "index.js",
  "author": "Airtune and Coranos",
  "license": "MIT",
  "keywords": [
    "banano"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BananoCoin/banano-ipfs.git"
  },
  "files": [
    "./index.js"
  ],
  "scripts": {
    "coverage": "nyc -x 'test/util/**' -x 'test/**' ./node_modules/mocha/bin/mocha -t 100000 test/",
    "docs": "jsdoc2md documentation.js index.js > docs/documentation.md",
    "eslint": "eslint --fix 'index.js';eslint --fix 'test/**/*.js'",
    "npm-check-updates": "npm-check-updates -u",
    "preflight": "npm update;npm run npm-check-updates;npm outdated;npm i;npm audit fix;npm run eslint;npm run prettier;npm run coverage;npm run docs;",
    "prettier-docs": "prettier \"docs/**/**.{ts,js,json,css,scss,html,md}\" --single-quote --write",
    "prettier-test": "prettier \"test/**/**.{ts,js,json,css,scss,html,md}\" --single-quote --write",
    "prettier-root": "prettier \"*.{ts,js,json,css,scss,html}\" --single-quote --write",
    "prettier": "npm run prettier-docs;npm run prettier-test;npm run prettier-root;",
    "test": "./node_modules/mocha/bin/mocha --recursive -t 100000 test/"
  },
  "dependencies": {
    "@bananocoin/bananojs": "^2.5.3",
    "bs58": "^5.0.0"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@coranos/jsdoc-to-markdown": "^7.1.3",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^9.2.1",
    "npm-check-updates": "^12.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1"
  }
}
