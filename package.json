    "bs58": "^5.0.0"
  },
  "preferGlobal": true,
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.33.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc-to-markdown": "^8.0.0",
    "mocha": "^10.2.0",
    "npm-check-updates": "^16.6.3",
    "nyc": "^15.1.0",
    "prettier": "^2.8.3"
